using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Fruitautomaat
{
    class Fruit
    {
        internal Apple Apples { get; set; }
        internal Pear Pears { get; set; }
        internal Int32 Amount { get; set; }
        internal Fruit(Apple apples, Pear pears, Int32 Amount)
        {
            Apples = apples;
            Pears = pears;
        }
    }
}
